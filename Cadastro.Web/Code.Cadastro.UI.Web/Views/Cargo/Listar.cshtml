@model List<Code.Cadastro.UI.Web.Models.Cargo>
@{
    ViewBag.Title = "Lista de cargos";
}

<h2>Listar</h2>
@if (@TempData["msg"] != null)
{
    <div class="alert alert-success">
        @TempData["msg"]
    </div>
}

<table class="table table-striped">

    <tr>
        <th>Titulo</th>
        <th>Salario</th>
        <th></th>
    </tr>
       @foreach (var cargo in Model)
       {
        <tr>
            <td>@cargo.Titulo</td>
            <td>@cargo.Salario</td>
            <td>
                @Html.ActionLink("Editar",
                "Editar", new { id = @cargo.CargoId },
                    new { @class = "btn btn-warning" })
                @Html.ActionLink("Excluir", "Excluir",
                    new { id = @cargo.CargoId },
                    new { @class = "btn btn-danger" })
            </td>
        </tr>
       }



</table>